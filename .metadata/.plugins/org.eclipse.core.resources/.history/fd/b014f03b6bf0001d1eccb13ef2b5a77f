package game;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int line = sc.nextInt();
		Player p1 = new Player(line);
		p1.MakingBoard();
		
		p1.PrintBoard();
		
		int select = sc.nextInt();
		
		p1.setSelect(select);
		p1.PrintBoard();
	}
}
