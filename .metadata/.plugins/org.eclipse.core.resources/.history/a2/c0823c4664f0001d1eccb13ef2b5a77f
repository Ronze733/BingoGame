package game;

public class Player {
	private int score = 0;
	private int bingo = 0;
	private int select;
	
	public Player() {
		// TODO Auto-generated constructor stub
	}

	public int getScore() {
		return score;
	}

	public void setScore(int score) {
		this.score += score;
	}

	public int getBingo() {
		return bingo;
	}

	public void setBingo(int bingo) {
		this.bingo += bingo;
	}

	public int getSelect() {
		return select;
	}

	public void setSelect(int select) {
		this.select = select;
	}

	
	
	
	
}
